Create table SALESMAN1

SALESMAN_ID VARCHAR(20) PRIMARY KEY,
NAME VARCHAR(26),
CITY VARCHAR(34),
COMMISSION NUMBER(4,2)
);
INSERT INTO SALESMAN1 VALUES('5001','JAMES HOOG','NEW YORK','0.15');
