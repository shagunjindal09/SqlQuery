SELECT * FROM SALESMAN1;
CREATE TABLE CUSTOMER
(
CUSTOMER_ID VARCHAR(20) PRIMARY KEY,
CUST_NAME VARCHAR(39),
CITY VARCHAR(36),
GRADE NUMBER(29,2),
SALESMAN_ID VARCHAR(20) REFERENCES SALESMAN1(SALESMAN_ID)

);
